<style>
  .search{
    display: flex;
    text-align: center;
    padding: 0.5rem;
}
  .search-bar{
    border-radius: 25px;
  }
  .search-btn i{
    display: inline;
    margin: auto;

  }
  .search-btn{
    border-radius: 25px;
    margin-left:4px;
  }
</style>
<nav class="navbar navbar-expand-md bg-body-tertiary border-bottom sticky-top">
  <div class="container-fluid">
    <a class="navbar-brand" href="/listings"><i class="fa-regular fa-compass"></i></a>
    <button
      class="navbar-toggler"
      type="button"
      data-bs-toggle="collapse"
      data-bs-target="#navbarNavAltMarkup"
      aria-controls="navbarNavAltMarkup"
    >
      <span class="navbar-toggler-icon"></span>
    </button>
    <div class="collapse navbar-collapse" id="navbarNavAltMarkup">
      <div class="navbar-nav ">
        
        <a class="nav-link" href="/listings">All listings</a>
        <a class="nav-link" href="/listings/new">Add listing</a>
      </div>
      <div class="search ms-auto">
        <input type="text" class="search-bar form-control"  placeholder="Search your Destination"/>
        <button class=" search-btn btn btn-danger"><i class="fa-solid fa-magnifying-glass"></i>Search</button>
      </div>
      <div class="navbar-nav ms-auto">
       <% if(!currUser)  {%>
        <a class="nav-link" aria-current="page" href="/signup"><b>Sign Up</b></a>
        <a class="nav-link" href="/login"><b>Login</b></a>
        <%}%>
      
      <%if(currUser){%>
        <a class="nav-link" href="/logout">Logout</a>
        <%}%>
      </div>
    </div>
  </div>
</nav>
